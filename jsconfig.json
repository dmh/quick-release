{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false,
    "moduleResolution": "nodenext",
    "baseUrl": "./",
    "paths": {
        "#*": ["./lib/*"]
    }
  },
  "include": ["lib/**/*", "bin/**/*"],
}
