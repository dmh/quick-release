{
  "name": "quick-release",
  "version": "2.1.0",
  "description": "Quickly generate and publish your application releases",
  "license": "MIT",
  "author": "dmh",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmh/quick-release.git"
  },
  "bin": {
    "quick-release": "./bin/qr-cli.js",
    "qr": "./bin/qr-cli.js"
  },
  "homepage": "https://dmh.github.io/quick-release",
  "bugs": {
    "url": "https://github.com/dmh/quick-release/issues"
  },
  "keywords": [
    "quick-release",
    "release",
    "publish",
    "github",
    "git",
    "changelog",
    "semver",
    "versioning",
    "semantic",
    "automatization",
    "terminal",
    "console",
    "cli",
    "shell",
    "command-line"
  ],
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint-js": "standard -v 'lib/**/*.js bin/**/*.js'",
    "check-js": "tsc -p jsconfig.json --maxNodeModuleJsDepth 0",
    "test": "npm run lint-js && npm run check-js",
    "docs": "jsdoc -c jsdoc.json"
  },
  "imports": {
    "#*": "./lib/*"
  },
  "dependencies": {
    "@inquirer/prompts": "2.3.0",
    "@octokit/request": "8.0.2",
    "chalk": "5.3.0",
    "dotenv": "16.3.1",
    "editorconfig": "2.0.0",
    "execa": "7.1.1",
    "git-url-parse": "13.1.0",
    "is-online": "10.0.0",
    "minimist": "1.2.8",
    "ora": "6.3.1",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@types/git-url-parse": "^9.0.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "~20.4.1",
    "@types/semver": "~7.5.0",
    "husky": "~8.0.3",
    "jsdoc": "~4.0.2",
    "modern-jsdoc-template": "~1.0.0",
    "standard": "~17.1.0",
    "typescript": "~5.1.6"
  },
  "engines": {
    "node": ">=18.16",
    "npm": ">=9.5.1"
  },
  "type": "module"
}
